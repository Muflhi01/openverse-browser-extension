{
  "name": "Openverse",
  "author": "Openverse",
  "description": "Search, filter, and use openly-licensed images.",
  "version": "2.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png",
    "128": "icons/icon-128.png"
  },
  "browser_action": {
    "default_title": "Openverse",
    "default_icon": {
      "16": "icons/icon-16.png",
      "24": "icons/icon-24.png",
      "32": "icons/icon-32.png"
    },
    "default_popup": "popup/popup.html"
  },
  "permissions": ["storage"],
  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  }
}
